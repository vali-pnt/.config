{
  "theme": "Carbonfox",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "buffer_line_height": { "custom": 1.3 },
  "soft_wrap": "editor_width",
  "features": {
    "copilot": false
  },

  "vim_mode": true,
  "relative_line_numbers": true,

  "auto_update": false,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
